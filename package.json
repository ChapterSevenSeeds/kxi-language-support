{
  "name": "kxi-language-support",
  "displayName": "KXI Language Support",
  "description": "Adds basic syntax highlighting for the KXI language used by CS 4490 at UVU.",
  "version": "0.0.5",
  "publisher": "chaptersevenseeds",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChapterSevenSeeds/kxi-language-support"
  },
  "activationEvents": [
    "onLanguage:kxi"
  ],
  "main": "index.js",
  "icon": "logos/Logo.png",
  "contributes": {
    "languages": [
      {
        "id": "kxi",
        "aliases": [
          "KXI",
          "kxi"
        ],
        "extensions": [
          ".kxi"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./logos/Logo-small.png",
          "dark": "./logos/Logo-small.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "kxi",
        "scopeName": "source.kxi",
        "path": "./syntaxes/kxi.tmLanguage.json"
      }
    ]
  }
}